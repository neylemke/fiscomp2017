    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "arquivo = open(\"blabla.txt\", 'r')  # abre o arquivo para leitura\n",
    "dict={}\n",
    "\n",
    "for i in arquivo.readlines():\n",
    "    for word in i.split():\n",
    "        if word in dict:\n",
    "            dict[word] = dict[word]+1\n",
    "        else:\n",
    "            dict[word]=1\n",
    "ic=dict.values()\n",
    "ic.sort()\n",
    "ic.reverse()\n",
    "X=np.array(range(len(ic)))\n",
    "Y=np.array(ic)\n",
    "logx = np.log(X)\n",
    "logy = np.log(Y)\n",
    "fig=plt.figure(figsize=(10,10))\n",
    "ax = fig.gca()\n",
    "x = fig.gca()\n",
    "ax.set_title('Frequencia de plavras da biblia')\n",
    "ax.set_xlabel('rank')\n",
    "ax.set_ylabel('Freq')\n",
    "plt.plot(logx,logy)"
   ]
